package br.com.fiap;

public class TiposPrimitivos {

	public static void main(String[] args) {

		char sexo = 'f';
		byte idade = 25;
		short codigo = 15650;
		float media = 8.7f;
		int alunos = 50, classes = 11;
		long brasileiros = 216535748;
		double dolar = 5.15;
		boolean alternativa = false;
		System.out.println("sexo: " + sexo + "\nidade: " + idade + "código: " + codigo);
		System.out.println("média: " + media + "alunos: " + alunos + "classes: " + classes);
		System.out.println("habitantes: " + brasileiros + "Dólar: " + dolar);
		System.out.println("alternativa: " + alternativa);

		
	}

}
